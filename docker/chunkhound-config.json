		{ "name": "district-administrator", "path": "/workspace/repos/district-administrator" },
		{ "name": "reports-2.0", "path": "/workspace/repos/reports-2.0" },
		{ "name": "upbeat-survey-administration", "path": "/workspace/repos/upbeat-survey-administration" },
		{ "name": "survey-administrator", "path": "/workspace/repos/survey-administrator" },
		{ "name": "user-administrator", "path": "/workspace/repos/user-administrator" },
		{ "name": "engagement-database", "path": "/workspace/repos/engagement-database" },
		{ "name": "survey", "path": "/workspace/repos/survey" },
		{ "name": "datapacks", "path": "/workspace/repos/datapacks" },
