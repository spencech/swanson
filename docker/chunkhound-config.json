		{ "name": "upbeat-sendgrid-webhook", "path": "/workspace/repos/upbeat-sendgrid-webhook" },
		{ "name": "upbeat-sendgrid-websocket", "path": "/workspace/repos/upbeat-sendgrid-websocket" },
		{ "name": "pdf-generator", "path": "/workspace/repos/pdf-generator" },
		{ "name": "google-presentations", "path": "/workspace/repos/google-presentations" },
		{ "name": "spawnee", "path": "/workspace/repos/spawnee" },
		{ "name": "upbeat-documentation", "path": "/workspace/repos/upbeat-documentation" }
	]
}
