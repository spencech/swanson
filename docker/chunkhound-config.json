{
	"description": "Swanson multi-repo ChunkHound configuration. Per-repo indexes allow independent updates and scoped queries.",
	"embedding": {
		"model": "text-embedding-3-small",
		"dimensions": 512
	},
	"ignore": [
		"node_modules",
		".git",
		"dist",
		"dist-electron",
		"release",
		"coverage",
		"*.min.js",
		"*.map",
		"package-lock.json",
		".angular",
		".nyc_output",
		"tmp",
		"temp"
	],
	"repos": [
		{ "name": "upbeat-aws-infrastructure", "path": "/workspace/repos/upbeat-aws-infrastructure" },
